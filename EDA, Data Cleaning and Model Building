      "[[  1 795]\n",
      " [  0  50]] \n",
      "\n"
     ]
    }
   ],
   "source": [
    "for i in threshold:\n",
    "    j = np.where(y_roc > i,1,0)\n",
    "    print('threshold:{}'.format(i))\n",
    "    print(confusion_matrix(Y_test,j),'\\n')"
   ]
